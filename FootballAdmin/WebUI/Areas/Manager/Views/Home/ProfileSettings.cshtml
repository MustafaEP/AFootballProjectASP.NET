                                        {
                                            <!--begin::Stats-->
                                            <div class="d-flex flex-wrap">
                                                <!--begin::Stat-->
                                                <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 me-6 mb-3">
                                                    <!--begin::Number-->
                                                    <div class="d-flex align-items-center">
                                                        <i class="ki-duotone ki-arrow-up fs-2 text-success me-2">
                                                            <span class="path1"></span>
                                                            <span class="path2"></span>
                                                        </i>
                                                        <div class="fs-2 fw-bold" data-kt-countup="true" data-kt-countup-value="@Model.ManagerClub.Budget" data-kt-countup-prefix="₺">0</div>
                                                    </div>
                                                    <!--end::Number-->
                                                    <!--begin::Label-->
                                                    <div class="fw-semibold fs-6 text-gray-400">Kulüp</div>
                                                    <!--end::Label-->
                                                </div>
                                                <!--end::Stat-->
                                                <!--begin::Stat-->
                                                <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 me-6 mb-3">
                                                    <!--begin::Number-->
                                                    <div class="d-flex align-items-center">
                                                        <i class="ki-duotone ki-arrow-down fs-2 text-danger me-2">
                                                            <span class="path1"></span>
                                                            <span class="path2"></span>
                                                        </i>
                                                        <div class="fs-2 fw-bold" data-kt-countup="true" data-kt-countup-value="10">0</div>
                                                    </div>
                                                    <!--end::Number-->
                                                    <!--begin::Label-->
                                                    <div class="fw-semibold fs-6 text-gray-400">Maç Sayısı</div>
                                                    <!--end::Label-->
                                                </div>
                                                <!--end::Stat-->
                                                <!--begin::Stat-->
                                                <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 me-6 mb-3">
                                                    <!--begin::Number-->
                                                    <div class="d-flex align-items-center">
                                                        <i class="ki-duotone ki-arrow-up fs-2 text-success me-2">
                                                            <span class="path1"></span>
                                                            <span class="path2"></span>
                                                        </i>
                                                        <div class="fs-2 fw-bold" data-kt-countup="true" data-kt-countup-value="60" data-kt-countup-prefix="%">0</div>
                                                    </div>
                                                    <!--end::Number-->
                                                    <!--begin::Label-->
                                                    <div class="fw-semibold fs-6 text-gray-400">Galibiyet Oranı</div>
                                                    <!--end::Label-->
                                                </div>
                                                <!--end::Stat-->
                                            </div>
                                            <!--end::Stats-->
                                        }
                                    </div>
                                    <!--end::Wrapper-->
                                    <!--begin::Progress-->
                                    <div class="d-flex align-items-center w-200px w-sm-300px flex-column mt-3">
                                        <div class="d-flex justify-content-between w-100 mt-auto mb-2">
                                            <span class="fw-semibold fs-6 text-gray-400">Profil Tamamlama</span>
                                            <span class="fw-bold fs-6">50%</span>
                                        </div>
                                        <div class="h-5px mx-3 w-100 bg-light mb-3">
                                            <div class="bg-success rounded h-5px" role="progressbar" style="width: 50%;" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                    </div>
                                    <!--end::Progress-->
                                </div>
                                <!--end::Stats-->
                            </div>
                            <!--end::Info-->
                        </div>
                        <!--end::Details-->
                        <!--begin::Navs-->
                        <ul class="nav nav-stretch nav-line-tabs nav-line-tabs-2x border-transparent fs-5 fw-bold">
                            <!--begin::Nav item-->
                            <li class="nav-item mt-2">
                                <a class="nav-link text-active-primary ms-0 me-10 py-5" href="/Manager/Home/Profile">Profil</a>
                            </li>
                            <!--end::Nav item-->
                            <!--begin::Nav item-->
                            <li class="nav-item mt-2">
                                <a class="nav-link text-active-primary ms-0 me-10 py-5 active" href="/Manager/Home/ProfileSettings">Profil Ayarları</a>
                            </li>
                            <!--end::Nav item-->
                            <!--end::Nav item-->
                        </ul>
                        <!--begin::Navs-->
                    </div>
                </div>
                <!--end::Navbar-->
                <!--end::Navbar-->
                <!--begin::Basic info-->
                <div class="card mb-5 mb-xl-10">
                    <!--begin::Card header-->
                    <div class="card-header border-0 cursor-pointer" role="button" data-bs-toggle="collapse" data-bs-target="#kt_account_profile_details" aria-expanded="true" aria-controls="kt_account_profile_details">
                        <!--begin::Card title-->
                        <div class="card-title m-0">
                            <h3 class="fw-bold m-0">Profil Güncelle</h3>
                        </div>
                        <!--end::Card title-->
                    </div>
                    <!--begin::Card header-->
                    <!--begin::Content-->
                    <div id="kt_account_settings_profile_details" class="collapse show">
                        <!--begin::Form-->
                        <form id="kt_account_profile_details_form" class="form">
                            <!--begin::Card body-->
                            <div class="card-body border-top p-9">

                                <!--begin::Input group-->
                                <div class="row mb-6">
                                    <!--begin::Label-->
                                    <label class="col-lg-4 col-form-label fw-semibold fs-6">Avatar</label>
                                    <!--end::Label-->
                                    <!--begin::Col-->
                                    <div class="col-lg-8">
                                        <!--begin::Image input-->
                                        <div class="image-input image-input-outline" data-kt-image-input="true" style="background-image: url('@Url.Content("~/saul_theme/dist/assets/media/svg/avatars/blank.svg")');">
                                            @{
                                                var avatarUrl = "~" + Model.Avatar;
                                            }

                                            <!--begin::Preview existing avatar-->
                                            <div class="image-input-wrapper w-125px h-125px" id="avatar_image_id" data-image-url = "@Model.Avatar"
                                                 style="background-image: url(@Model.Avatar)">
                                                <!--end::Preview existing avatar-->
                                                <!--begin::Label-->
                                                <label class="btn btn-icon btn-circle btn-active-color-primary w-25px h-25px bg-body shadow" data-kt-image-input-action="change" data-bs-toggle="tooltip" title="Avatarı Değiştir">
                                                    <i class="ki-duotone ki-pencil fs-7">
                                                        <span class="path1"></span>
                                                        <span class="path2"></span>
                                                    </i>
                                                    <!--begin::Inputs-->
                                                    <input type="button" id="avatar_add_id" name="avatar" />
                                                    <input type="hidden" id="avatar_remove_id" name="avatar_remove" />
                                                    <!--end::Inputs-->
                                                </label>
                                                <!--end::Label-->
                                                <!--begin::Cancel-->
                                                <span class="btn btn-icon btn-circle btn-active-color-primary w-25px h-25px bg-body shadow" data-kt-image-input-action="cancel" data-bs-toggle="tooltip" title="Cancel avatar">
                                                    <i class="ki-duotone ki-cross fs-2">
                                                        <span class="path1"></span>
                                                        <span class="path2"></span>
                                                    </i>
                                                </span>
                                                <!--end::Cancel-->
                                                <!--begin::Remove-->
                                                <span class="btn btn-icon btn-circle btn-active-color-primary w-25px h-25px bg-body shadow" data-kt-image-input-action="remove" data-bs-toggle="tooltip" title="Remove avatar">
                                                    <i class="ki-duotone ki-cross fs-2">
                                                        <span class="path1"></span>
                                                        <span class="path2"></span>
                                                    </i>
                                                </span>
                                                <!--end::Remove-->
                                            </div>
                                            <!--end::Image input-->
                                        </div>
                                        <!--end::Col-->
                                    </div>
                                    <!--end::Input group-->
                                    <!--begin::Input group-->
                                    <div class="row mb-6">
                                        <!--begin::Label-->
                                        <label class="col-lg-4 col-form-label required fw-semibold fs-6">Ad Soyad</label>
                                        <!--end::Label-->
                                        <!--begin::Col-->
                                        <div class="col-lg-8">
                                            <!--begin::Row-->
                                            <div class="row">
                                                <!--begin::Col-->
                                                <div class="col-lg-6 fv-row">
                                                    <input type="text" name="fname" id="nameSettingsId" class="form-control form-control-lg form-control-solid mb-3 mb-lg-0" placeholder="First name" value="@Model.Name" />
                                                </div>
                                                <!--end::Col-->
                                                <!--begin::Col-->
                                                <div class="col-lg-6 fv-row">
                                                    <input type="text" name="lname" id="surnameSettingsId" class="form-control form-control-lg form-control-solid" placeholder="Last name" value="@Model.SurName" />
                                                </div>
                                                <!--end::Col-->
                                            </div>
                                            <!--end::Row-->
                                        </div>
                                        <!--end::Col-->
                                    </div>
                                    <!--end::Input group-->
                                    <!--begin::Input group-->
                                    <div class="row mb-6">
                                        <!--begin::Label-->
                                        <label class="col-lg-4 col-form-label required fw-semibold fs-6">E-Mail</label>
                                        <!--end::Label-->
                                        <!--begin::Col-->
                                        <div class="col-lg-8 fv-row">
                                            <input type="email" name="company" id="emailSettingsId" class="form-control form-control-lg form-control-solid" placeholder="Company name" value="@Model.Email" />
                                        </div>
                                        <!--end::Col-->
                                    </div>
                                    <!--end::Input group-->
                                    <!--begin::Input group-->
                                    <div class="row mb-6">
                                        <!--begin::Label-->
                                        <label class="col-lg-4 col-form-label fw-semibold fs-6">
                                            <span class="required">Telefon Numarası</span>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="Telefon Numarası Geçerli Olmalıdır.">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                        </label>
                                        <!--end::Label-->
                                        <!--begin::Col-->
                                        <div class="col-lg-8 fv-row">
                                            <input type="tel" name="phone" id="phone_number_id" class="form-control form-control-lg form-control-solid" placeholder="Phone number" value="@Model.Phone" />
                                        </div>
                                        <!--end::Col-->
                                    </div>
                                    <!--end::Input group-->
                                    <!--begin::Input group-->
                                    <div class="row mb-6">
                                        <!--begin::Label-->
                                        <label class="col-lg-4 col-form-label fw-semibold fs-6">
                                            <span class="required">Ülke</span>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="Ülke">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                        </label>
                                        <!--end::Label-->
                                        <!--begin::Col-->
                                        <div class="col-lg-8 fv-row">
                                            <select name="country" aria-label="Ülke Seç" id="countrySettingsId" data-control="select2" data-placeholder="Select a country..." class="form-select form-select-solid form-select-lg fw-semibold">
                                                <option value="">Bir Ülke Seç...</option>
                                                <option data-kt-flag="flags/argentina.svg" value="Argentina" @(Model.County == "Argentina" ? "selected" : "")>Arjantin</option>
                                                <option data-kt-flag="flags/australia.svg" value="Australia" @(Model.County == "Australia" ? "selected" : "")>Avustralya</option>
                                                <option data-kt-flag="flags/azerbaijan.svg" value="Azerbaijan" @(Model.County == "Azerbaijan" ? "selected" : "")>Azerbeycan</option>
                                                <option data-kt-flag="flags/brazil.svg" value="Brazil" @(Model.County == "Brazil" ? "selected" : "")>Brezilya</option>
                                                <option data-kt-flag="flags/bulgaria.svg" value="Bulgaria" @(Model.County == "Bulgaria" ? "selected" : "")>Bulgaristan</option>
                                                <option data-kt-flag="flags/canada.svg" value="Canada" @(Model.County == "Canada" ? "selected" : "")>Kanada</option>
                                                <option data-kt-flag="flags/china.svg" value="China" @(Model.County == "China" ? "selected" : "")>Çin</option>
                                                <option data-kt-flag="flags/france.svg" value="France" @(Model.County == "France" ? "selected" : "")>Fransa</option>
                                                <option data-kt-flag="flags/germany.svg" value="Germany" @(Model.County == "Germany" ? "selected" : "")>Almanya</option>
                                                <option data-kt-flag="flags/greece.svg" value="Greece" @(Model.County == "Greece" ? "selected" : "")>Yunanistan</option>
                                                <option data-kt-flag="flags/india.svg" value="India" @(Model.County == "India" ? "selected" : "")>Hindistan</option>
                                                <option data-kt-flag="flags/ireland.svg" value="Ireland" @(Model.County == "Ireland" ? "selected" : "")>İrlenda</option>
                                                <option data-kt-flag="flags/italy.svg" value="Italy" @(Model.County == "Italy" ? "selected" : "")>Italy</option>
                                                <option data-kt-flag="flags/japan.svg" value="Japan" @(Model.County == "Japan" ? "selected" : "")>Japonya</option>
                                                <option data-kt-flag="flags/mexico.svg" value="Mexico" @(Model.County == "Mexico" ? "selected" : "")>Meksika</option>
                                                <option data-kt-flag="flags/netherlands.svg" value="Netherlands" @(Model.County == "Netherlands" ? "selected" : "")>Hollanda</option>
                                                <option data-kt-flag="flags/spain.svg" value="Spain" @(Model.County == "Spain" ? "selected" : "")>İspanya</option>
                                                <option data-kt-flag="flags/sweden.svg" value="Sweden" @(Model.County == "Sweden" ? "selected" : "")>İsveç</option>
                                                <option data-kt-flag="flags/switzerland.svg" value="CH" @(Model.County == "CH" ? "selected" : "")>Switzerland</option>
